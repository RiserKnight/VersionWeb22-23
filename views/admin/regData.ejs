<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="icon" href="/images/vlogo.png" type="image/icon type">
   
    <title>CO-Admin Panel</title>
</head>
<body>
    <section>
        <section class="main-content">
   
            <!-- Portfolio -->
            <div class="col-md-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4><b>Table</b></h4>
                        <a href="/Version@2023/coadmin"><button>CO-Admin Panel</button></a>
                    </div>
                    <div class="table-responsive">
                        <%if(users[0]){%>
                        <table class="table table-bordered table-hover">
                            <thead id="stockNameRow">
                                <%const arrH = Object.keys(users[0]);%>
                                <tr>
                                    <th>
                                        Serial
                                    </th>
                                    <%arrH.forEach((head)=>{%>
                                    <th style="color: rgb(95, 98, 95);">
                                        <h4><%=head%></h4>
                                    </th>
                                    <%})%>
                                    <th>Remove</th>
                                </tr>
                            </thead>

                            <tbody>
                            <%var a=1%>
                            <%users.forEach((user)=>{%>
                                <%const arr = Object.values(user); %>
                                
                                <tr>
                                    <td>
                                        <%=a%>
                                    </td>
                                    <%arr.forEach((data)=>{%>
                                    <td>
                                        <h5> <b><%=data%></b></h5>
                                    </td>
                                    <%})%>
                                    <form action=`/unregister/event/${arr[0]}` method="post">
                                        <td>
                                            <input style="width: auto;
                                            background-color: #698ee5;
                                            color: white;
                                            padding: 14px 20px;
                                            margin: 8px 0;
                                            border: none;
                                            border-radius: 8px;
                                            cursor: pointer;" type="submit" value="Remove">
                                        </td>
                                    </form>
                                    
                                </tr>
                             <%a++})%>
                            </tbody>
                        </table>
                        <%}else{%>
                            <h1>Empty</h1>
                        <%}%>
                    </div>
                </div>
            </div>
            </div>
        </section>
    </section>
        
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</html>