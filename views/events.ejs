<body>
    <%- include('partials/nav') %>
    <!-- <div class="eve-container"> -->
        <h1 id="teams-heading" class="text-center">version'23 <span>Events</span></h1>      
        <div class="card-container">
        </div>
        <div id="myModal" class="modal">

            <!-- Modal content -->
            <div class="modal-content">
                <h4 id="evename"></h4>
                <p id="evedet">Some text in the Modal..</p>
                <div class="d-flex btns">
                    <input type="text" id="eventID" style="display: none;">
                    <button type="submit" class="btn btn-sm mx-1 rounded-pill px-3" id="btn-reg">Register</button>
                    <button type="button"  class="btn btn-sm btn-light mx-1 rounded-pill px-2 close"><span>close</span></button>
                </div>
            </div>
          
        </div>
    <!-- </div> -->
    <!-- modal for displaying alerts -->
    
    <input type="text" id="registerData" style="display: none;" value="<%=JSON.stringify(registerData)%>">
    <script  type="module" src="/res/eventpage.js"></script>
    <script src="/res/navigation.js"></script>
